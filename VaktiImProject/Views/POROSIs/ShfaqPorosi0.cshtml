@model IEnumerable<VaktiImProject.Models.POROSI>

@{
    ViewBag.Title = "Porosit e anulluara";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Shfaq Porosit e anulluara </title>
</head>
<body>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.datetime_Porosi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.status_porosie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ADRESA.rruga)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PERDORUE.emri)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PERDORUE1.emri)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.datetime_Porosi)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.status_porosie)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ADRESA.rruga)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PERDORUE.emri)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PERDORUE1.emri)
                </td>
                <td>

                    @Html.ActionLink("Details", "Details", new { id = item.porosi_id })

                </td>
            </tr>
        }

    </table>
</body>
</html>
<div>
    @Html.ActionLink("Back to List", "Index", "GATIM")
</div>
